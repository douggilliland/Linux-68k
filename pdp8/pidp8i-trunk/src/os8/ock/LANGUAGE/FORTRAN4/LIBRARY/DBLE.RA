/	SUBROUTINE DBLE - REAL TO DBL PREC
/
/ VERSION 5A 4-27-77  PT
/
	SECT	DBLE
	BASE	0
	DPCHK
	FLDA	0
	STARTD
	FSTA	RETRN		/SAVE RETURN
	FADD	TWO		/ADDR OF ARG POINTER
	FSTA	3
	FLDA%	3		/ADDR OF ARG
	FSTA	3
	STARTF
	FLDA%	3		/GET ARG
	FSTA	DTEMP
	FCLA
	FSTA	DTEMP+3		/0 FOR LAST 3 WORDS
	STARTE
	FLDA	DTEMP
RETRN,	0;0			/FLOAT IS A NOP
TWO,	0;2
DTEMP,	F 0.0
	F 0.0
	END
