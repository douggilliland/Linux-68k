  86 1094 0600 0001 	    ADDI.B  #0x01,%D0
  87 1098 06B9 0000 	    ADDI.L  #4096,A0
  87      1000 0000 
  87      0000 
  88 10a2 B3C8      	    CMP.L   %A0,%A1
  89 10a4 6C02      	    BGE.S   DONE4K
  90 10a6 60E4      	    BRA.S   CHK4K
  91               	DONE4K:
  92               	
  93               	* CHECK NEXT 4 SRAM ADDRESS LINES (A8..A11 ON SRAM)
  94               	* A12-A19 = 0
  95               	* WRITE AN INCREMENTING VALUE EVERY 256 BYTES AND VERIFY
  96               	* ONLY NEED 16 UNIQUE PATTERNS FOR 4 BITS
  97 10a8 7000      	    MOVEQ   #0x0,%D0
  98 10aa 207C 0030 		MOVE.L  #RAMSTART,%A0
  98      0000 
