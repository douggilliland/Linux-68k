F00:9046       *************************************************************************************
F00:9047       * EhBASIC keywords quick reference list								*
F00:9048       *************************************************************************************
F00:9049       
F00:9050       * glossary
F00:9051       
F00:9052       *		<.>		  required
F00:9053       *		{.|.}		  one of required
F00:9054       *		[.]		  optional
F00:9055       *		...		  may repeat as last
F00:9056       
F00:9057       *		any		= anything
F00:9058       *		num		= number
F00:9059       *		state		= statement
F00:9060       *		n		= positive integer
F00:9061       *		str		= string
F00:9062       *		var		= variable
F00:9063       *		nvar		= numeric variable
F00:9064       *		svar		= string variable
F00:9065       *		expr		= expression
F00:9066       *		nexpr		= numeric expression
F00:9067       *		sexpr		= string expression
F00:9068       
F00:9069       * statement separator
F00:9070       
F00:9071       * :		. [<state>] : [<state>]						* done
F00:9072       
F00:9073       * number bases
F00:9074       
F00:9075       * %		. %<binary num>							* done
F00:9076       * $		. $<hex num>							* done
F00:9077       
F00:9078       * commands
F00:9079       
F00:9080       * END		. END									* done
F00:9081       * FOR		. FOR <nvar>=<nexpr> TO <nexpr> [STEP <nexpr>]		* done
F00:9082       * NEXT	. NEXT [<nvar>[,<nvar>]...]					* done
F00:9083       * DATA	. DATA [{num|["]str["]}[,{num|["]str["]}]...]		* done
F00:9084       * INPUT	. INPUT [<">str<">;] <var>[,<var>[,<var>]...]		* done
F00:9085       * DIM		. DIM <var>(<nexpr>[,<nexpr>[,<nexpr>]])			* done
F00:9086       * READ	. READ <var>[,<var>[,<var>]...]				* done
F00:9087       * LET		. [LET] <var>=<expr>						* done
F00:9088       * DEC		. DEC <nvar>[,<nvar>[,<nvar>]...]				* done
F00:9089       * GOTO	. GOTO <n>								* done
F00:9090       * RUN		. RUN [<n>]								* done
F00:9091       * IF		. IF <expr>{GOTO<n>|THEN<{n|comm}>}[ELSE <{n|comm}>]	* done
F00:9092       * RESTORE	. RESTORE [<n>]							* done
F00:9093       * GOSUB	. GOSUB <n>								* done
F00:9094       * RETURN	. RETURN								* done
F00:9095       * REM		. REM [<any>]							* done
F00:9096       * STOP	. STOP								* done
F00:9097       * ON		. ON <nexpr>{GOTO|GOSUB}<n>[,<n>[,<n>]...]		* done
F00:9098       * NULL	. NULL <nexpr>							* done
F00:9099       * INC		. INC <nvar>[,<nvar>[,<nvar>]...]				* done
F00:9100       * WAIT	. WAIT <nexpr>,<nexpr>[,<nexpr>]				* done
F00:9101       * LOAD	. LOAD [<sexpr>]							* done for sim
F00:9102       * SAVE	. SAVE [<sexpr>][,[<n>][-<n>]]				* done for sim
F00:9103       * DEF		. DEF FN<var>(<var>)=<expr>					* done
F00:9104       * POKE	. POKE <nexpr>,<nexpr>						* done
F00:9105       * DOKE	. DOKE <nexpr>,<nexpr>						* done
F00:9106       * LOKE	. LOKE <nexpr>,<nexpr>						* done
F00:9107       * CALL	. CALL <nexpr>							* done
F00:9108       * DO		. DO									* done
F00:9109       * LOOP	. LOOP [{WHILE|UNTIL}<nexpr>]					* done
F00:9110       * PRINT	. PRINT [{;|,}][<expr>][{;|,}[<expr>]...]			* done
F00:9111       * CONT	. CONT								* done
F00:9112       * LIST	. LIST [<n>][-<n>]						* done
F00:9113       * CLEAR	. CLEAR								* done
F00:9114       * NEW		. NEW									* done
F00:9115       * WIDTH	. WIDTH [<n>][,<n>]						* done
F00:9116       * GET		. GET <var>								* done
F00:9117       * SWAP	. SWAP <var>,<var>						* done
F00:9118       * BITSET	. BITSET <nexpr>,<nexpr>					* done
F00:9119       * BITCLR	. BITCLR <nexpr>,<nexpr>					* done
F00:9120       
F00:9121       * sub commands (may not start a statement)
F00:9122       
F00:9123       * TAB		. TAB(<nexpr>)							* done
F00:9124       * ELSE	. IF <expr>{GOTO<n>|THEN<{n|comm}>}[ELSE <{n|comm}>]	* done
F00:9125       * TO		. FOR <nvar>=<nexpr> TO <nexpr> [STEP <nexpr>]		* done
F00:9126       * FN		. FN <var>(<expr>)						* done
F00:9127       * SPC		. SPC(<nexpr>)							* done
F00:9128       * THEN	. IF <nexpr> {THEN <{n|comm}>|GOTO <n>}			* done
F00:9129       * NOT		. NOT <nexpr>							* done
F00:9130       * STEP	. FOR <nvar>=<nexpr> TO <nexpr> [STEP <nexpr>]		* done
F00:9131       * UNTIL	. LOOP [{WHILE|UNTIL}<nexpr>]					* done
F00:9132       * WHILE	. LOOP [{WHILE|UNTIL}<nexpr>]					* done
F00:9133       
F00:9134       * operators
F00:9135       
F00:9136       * +		. [expr] + <expr>							* done
F00:9137       * -		. [nexpr] - <nexpr>						* done
F00:9138       * *		. <nexpr> * <nexpr>						* done fast hardware
F00:9139       * /		. <nexpr> / <nexpr>						* done fast hardware
F00:9140       * ^		. <nexpr> ^ <nexpr>						* done
F00:9141       * AND		. <nexpr> AND <nexpr>						* done
F00:9142       * EOR		. <nexpr> EOR <nexpr>						* done
F00:9143       * OR		. <nexpr> OR <nexpr>						* done
F00:9144       * >>		. <nexpr> >> <nexpr>						* done
F00:9145       * <<		. <nexpr> << <nexpr>						* done
F00:9146       
F00:9147       * compare functions
F00:9148       
F00:9149       * <		. <expr> < <expr>							* done
F00:9150       * =		. <expr> = <expr>							* done
F00:9151       * >		. <expr> > <expr>							* done
F00:9152       
F00:9153       * functions
F00:9154       
F00:9155       * SGN		. SGN(<nexpr>)							* done
F00:9156       * INT		. INT(<nexpr>)							* done
F00:9157       * ABS		. ABS(<nexpr>)							* done
F00:9158       * USR		. USR(<expr>)							* done
F00:9159       * FRE		. FRE(<expr>)							* done
F00:9160       * POS		. POS(<expr>)							* done
F00:9161       * SQR		. SQR(<nexpr>)							* done fast shift/sub
F00:9162       * RND		. RND(<nexpr>)							* done 32 bit PRNG
F00:9163       * LOG		. LOG(<nexpr>)							* done fast cordic
F00:9164       * EXP		. EXP(<nexpr>)							* done fast cordic
F00:9165       * COS		. COS(<nexpr>)							* done fast cordic
F00:9166       * SIN		. SIN(<nexpr>)							* done fast cordic
F00:9167       * TAN		. TAN(<nexpr>)							* done fast cordic
F00:9168       * ATN		. ATN(<nexpr>)							* done fast cordic
F00:9169       * PEEK	. PEEK(<nexpr>)							* done
F00:9170       * DEEK	. DEEK(<nexpr>)							* done
F00:9171       * LEEK	. LEEK(<nexpr>)							* done
F00:9172       * LEN		. LEN(<sexpr>)							* done
F00:9173       * STR$	. STR$(<nexpr>)							* done
F00:9174       * VAL		. VAL(<sexpr>)							* done
F00:9175       * ASC		. ASC(<sexpr>)							* done
F00:9176       * UCASE$	. UCASE$(<sexpr>)							* done
F00:9177       * LCASE$	. LCASE$(<sexpr>)							* done
F00:9178       * CHR$	. CHR$(<nexpr>)							* done
F00:9179       * HEX$	. HEX$(<nexpr>)							* done
F00:9180       * BIN$	. BIN$(<nexpr>)							* done
F00:9181       * BTST	. BTST(<nexpr>,<nexpr>)						* done
F00:9182       * MAX		. MAX(<nexpr>[,<nexpr>[,<nexpr>]...])			* done
F00:9183       * MIN		. MIN(<nexpr>[,<nexpr>[,<nexpr>]...])			* done
F00:9184       * PI		. PI									* done
F00:9185       * TWOPI	. TWOPI								* done
F00:9186       * VARPTR	. VARPTR(<var>)							* done
F00:9187       * SADD	. SADD(<svar>)							* done
F00:9188       * LEFT$	. LEFT$(<sexpr>,<nexpr>)					* done
F00:9189       * RIGHT$	. RIGHT$(<sexpr>,<nexpr>)					* done
F00:9190       * MID$	. MID$(<sexpr>,<nexpr>[,<nexpr>])				* done
F00:9191       * USING$	. USING$(<sexpr>,<nexpr>[,<nexpr>]...])			* done
