F00:0105       *************************************************************************************
F00:0106       *
F00:0107       * the following code is simulator specific, change to suit your system
F00:0108       
F00:0109       * output character to the console from register d0.b
F00:0110       
F00:0111       VEC_OUT
F00:0112               MOVEM.L  A0/D1,-(A7)    * Save working registers
               S02:00000802:  48 E7 40 80
F00:0113               LEA.L    SRA,A0     	* A0 points to console DUART
               S02:00000806:  41 F9 00 0F 00 02
F00:0114       TXNOTREADY
F00:0115               MOVE.B   (A0),D1        * Read DUART status
               S02:0000080C:  12 10
F00:0116               BTST     #2,D1          * Test Tx RDY bit
               S02:0000080E:  08 01 00 02
F00:0117               BEQ.S    TXNOTREADY     * Until DUART Tx ready
               S02:00000812:  67 F8
F00:0118               MOVE.B   D0,TBA       	* Write character to send
               S02:00000814:  13 C0 00 0F 00 06
F00:0119               MOVEM.L  (A7)+,A0/D1    * Restore working registers
               S02:0000081A:  4C DF 01 02
F00:0120               RTS
               S02:0000081E:  4E 75
F00:0121       
F00:0122       *************************************************************************************
F00:0123       *
F00:0124       * input a character from the console into register d0
F00:0125       * else return Cb=0 if there's no character available
F00:0126       
F00:0127       VEC_IN
F00:0128       	MOVEM.L  A0/D1,-(A7)    * Save working registers
               S02:00000820:  48 E7 40 80
F00:0129           LEA.L    SRA,A0      	* A0 points to console DUART
               S02:00000824:  41 F9 00 0F 00 02
F00:0130           MOVE.B   (A0),D1        * Read DUART status
               S02:0000082A:  12 10
F00:0131           BTST     #0,D1          * Test Rx Data Ready bit
               S02:0000082C:  08 01 00 00
F00:0132           BEQ.S    RXNOTREADY     * Branch If DUART Rx not ready
               S02:00000830:  67 10
F00:0133           MOVE.B   RBA,D0			* Read character received
               S02:00000832:  10 39 00 0F 00 06
F00:0134           MOVEM.L  (A7)+,A0/D1    * Restore working registers
               S02:00000838:  4C DF 01 02
F00:0135       	ORI.b	 #1,CCR	        * Set the carry, flag we got a byte
               S02:0000083C:  00 3C 00 01
F00:0136           RTS                     * Return
               S02:00000840:  4E 75
F00:0137       RXNOTREADY:
F00:0138           MOVEM.L  (A7)+,A0/D1    * Restore working registers
               S02:00000842:  4C DF 01 02
F00:0139       	ANDI.b	#$FE,CCR		* Clear the carry, flag character available
               S02:00000846:  02 3C 00 FE
F00:0140       	RTS
               S02:0000084A:  4E 75
