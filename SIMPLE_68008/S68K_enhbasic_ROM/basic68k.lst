F00:0164       code_start
F00:0165       *                            * Set up ACIA parameters
F00:0166       *    MOVE.b  #$30,CRA       * Reset Transmitter
F00:0167       *    MOVE.b  #$20,CRA       * Reset Receiver
F00:0168       *    MOVE.b  #$10,CRA       * Reset Mode Register Pointer
F00:0169           
F00:0170       *    MOVE.b  #$00,ACR       * Baud Rate Set #2
F00:0171       *    MOVE.b  #$CC,CSRA      * Set Tx and Rx rates to 38400
F00:0172       *    MOVE.b  #$93,MRA       * 7-bit, No Parity (0x93 for 8-bit, 0x92 for 7-bit)
F00:0173       *    MOVE.b  #$27,MRA       * Normal Mode, Using CTS/RTS, 1 stop bit
F00:0174           
F00:0175       *    MOVE.b  #$05,CRA       * Enable Transmit/Recieve
F00:0176       
F00:0177       *    MOVE.b  #$30,CRB       * Reset Transmitter
F00:0178       *    MOVE.b  #$20,CRB       * Reset Receiver
F00:0179       *    MOVE.b  #$10,CRB       * Reset Mode Register Pointer
F00:0180           
F00:0181       *    MOVE.b  #$CC,CSRB      * Set Tx and Rx rates to 38400
F00:0182       *    MOVE.b  #$93,MRB       * 7-bit, No Parity (0x93 for 8-bit, 0x92 for 7-bit)
F00:0183       *    MOVE.b  #$27,MRB       * Normal Mode, Using CTS/RTS, 1 stop bit
F00:0184           
F00:0185       *    MOVE.b  #$05,CRB       * Enable Transmit/Recieve
F00:0186       	
F00:0187       *	MOVE.b	#$00,OPC		* Output port configuration (all bit are outs)
F00:0188       *	MOVE.b	#$FC,OPR		* Clear all outputs
F00:0189       	move.b	#$03,OPS		* Drive handshake outs low (active)
               S02:00084858:  13 FC 00 03 00 0F 00 1C
F00:0190       * Note that RTS* must be asserted initially manually - after that
F00:0191       * RTS* is asserted auomatically whenever the receiver is ready to
F00:0192       * receive more data. Note also that the contents of the DUARTs
F00:0193       * output port register are inverted before they are fed to the output
F00:0194       * pins. That is, to assert RTS* low, it is necessary to load a one
F00:0195       * into the appropriate bit of the OPR.
F00:0196       
F00:0197       	MOVE.b	#'1',d0			* Got here
               S02:00084860:  10 3C 00 31
F00:0198       	bsr		VEC_OUT
               S02:00084864:  61 9C
