F00:9055       *************************************************************************************
F00:9056       * EhBASIC keywords quick reference list								*
F00:9057       *************************************************************************************
F00:9058       
F00:9059       * glossary
F00:9060       
F00:9061       *		<.>		  required
F00:9062       *		{.|.}		  one of required
F00:9063       *		[.]		  optional
F00:9064       *		...		  may repeat as last
F00:9065       
F00:9066       *		any		= anything
F00:9067       *		num		= number
F00:9068       *		state		= statement
F00:9069       *		n		= positive integer
F00:9070       *		str		= string
F00:9071       *		var		= variable
F00:9072       *		nvar		= numeric variable
F00:9073       *		svar		= string variable
F00:9074       *		expr		= expression
F00:9075       *		nexpr		= numeric expression
F00:9076       *		sexpr		= string expression
F00:9077       
F00:9078       * statement separator
F00:9079       
F00:9080       * :		. [<state>] : [<state>]						* done
F00:9081       
F00:9082       * number bases
F00:9083       
F00:9084       * %		. %<binary num>							* done
F00:9085       * $		. $<hex num>							* done
F00:9086       
F00:9087       * commands
F00:9088       
F00:9089       * END		. END									* done
F00:9090       * FOR		. FOR <nvar>=<nexpr> TO <nexpr> [STEP <nexpr>]		* done
F00:9091       * NEXT	. NEXT [<nvar>[,<nvar>]...]					* done
F00:9092       * DATA	. DATA [{num|["]str["]}[,{num|["]str["]}]...]		* done
F00:9093       * INPUT	. INPUT [<">str<">;] <var>[,<var>[,<var>]...]		* done
F00:9094       * DIM		. DIM <var>(<nexpr>[,<nexpr>[,<nexpr>]])			* done
F00:9095       * READ	. READ <var>[,<var>[,<var>]...]				* done
F00:9096       * LET		. [LET] <var>=<expr>						* done
F00:9097       * DEC		. DEC <nvar>[,<nvar>[,<nvar>]...]				* done
F00:9098       * GOTO	. GOTO <n>								* done
F00:9099       * RUN		. RUN [<n>]								* done
F00:9100       * IF		. IF <expr>{GOTO<n>|THEN<{n|comm}>}[ELSE <{n|comm}>]	* done
F00:9101       * RESTORE	. RESTORE [<n>]							* done
F00:9102       * GOSUB	. GOSUB <n>								* done
F00:9103       * RETURN	. RETURN								* done
F00:9104       * REM		. REM [<any>]							* done
F00:9105       * STOP	. STOP								* done
F00:9106       * ON		. ON <nexpr>{GOTO|GOSUB}<n>[,<n>[,<n>]...]		* done
F00:9107       * NULL	. NULL <nexpr>							* done
F00:9108       * INC		. INC <nvar>[,<nvar>[,<nvar>]...]				* done
F00:9109       * WAIT	. WAIT <nexpr>,<nexpr>[,<nexpr>]				* done
F00:9110       * LOAD	. LOAD [<sexpr>]							* done for sim
F00:9111       * SAVE	. SAVE [<sexpr>][,[<n>][-<n>]]				* done for sim
F00:9112       * DEF		. DEF FN<var>(<var>)=<expr>					* done
F00:9113       * POKE	. POKE <nexpr>,<nexpr>						* done
F00:9114       * DOKE	. DOKE <nexpr>,<nexpr>						* done
F00:9115       * LOKE	. LOKE <nexpr>,<nexpr>						* done
F00:9116       * CALL	. CALL <nexpr>							* done
F00:9117       * DO		. DO									* done
F00:9118       * LOOP	. LOOP [{WHILE|UNTIL}<nexpr>]					* done
F00:9119       * PRINT	. PRINT [{;|,}][<expr>][{;|,}[<expr>]...]			* done
F00:9120       * CONT	. CONT								* done
F00:9121       * LIST	. LIST [<n>][-<n>]						* done
F00:9122       * CLEAR	. CLEAR								* done
F00:9123       * NEW		. NEW									* done
F00:9124       * WIDTH	. WIDTH [<n>][,<n>]						* done
F00:9125       * GET		. GET <var>								* done
F00:9126       * SWAP	. SWAP <var>,<var>						* done
F00:9127       * BITSET	. BITSET <nexpr>,<nexpr>					* done
F00:9128       * BITCLR	. BITCLR <nexpr>,<nexpr>					* done
F00:9129       
F00:9130       * sub commands (may not start a statement)
F00:9131       
F00:9132       * TAB		. TAB(<nexpr>)							* done
F00:9133       * ELSE	. IF <expr>{GOTO<n>|THEN<{n|comm}>}[ELSE <{n|comm}>]	* done
F00:9134       * TO		. FOR <nvar>=<nexpr> TO <nexpr> [STEP <nexpr>]		* done
F00:9135       * FN		. FN <var>(<expr>)						* done
F00:9136       * SPC		. SPC(<nexpr>)							* done
F00:9137       * THEN	. IF <nexpr> {THEN <{n|comm}>|GOTO <n>}			* done
F00:9138       * NOT		. NOT <nexpr>							* done
F00:9139       * STEP	. FOR <nvar>=<nexpr> TO <nexpr> [STEP <nexpr>]		* done
F00:9140       * UNTIL	. LOOP [{WHILE|UNTIL}<nexpr>]					* done
F00:9141       * WHILE	. LOOP [{WHILE|UNTIL}<nexpr>]					* done
F00:9142       
F00:9143       * operators
F00:9144       
F00:9145       * +		. [expr] + <expr>							* done
F00:9146       * -		. [nexpr] - <nexpr>						* done
F00:9147       * *		. <nexpr> * <nexpr>						* done fast hardware
F00:9148       * /		. <nexpr> / <nexpr>						* done fast hardware
F00:9149       * ^		. <nexpr> ^ <nexpr>						* done
F00:9150       * AND		. <nexpr> AND <nexpr>						* done
F00:9151       * EOR		. <nexpr> EOR <nexpr>						* done
F00:9152       * OR		. <nexpr> OR <nexpr>						* done
F00:9153       * >>		. <nexpr> >> <nexpr>						* done
F00:9154       * <<		. <nexpr> << <nexpr>						* done
F00:9155       
F00:9156       * compare functions
F00:9157       
F00:9158       * <		. <expr> < <expr>							* done
F00:9159       * =		. <expr> = <expr>							* done
F00:9160       * >		. <expr> > <expr>							* done
F00:9161       
F00:9162       * functions
F00:9163       
F00:9164       * SGN		. SGN(<nexpr>)							* done
F00:9165       * INT		. INT(<nexpr>)							* done
F00:9166       * ABS		. ABS(<nexpr>)							* done
F00:9167       * USR		. USR(<expr>)							* done
F00:9168       * FRE		. FRE(<expr>)							* done
F00:9169       * POS		. POS(<expr>)							* done
F00:9170       * SQR		. SQR(<nexpr>)							* done fast shift/sub
F00:9171       * RND		. RND(<nexpr>)							* done 32 bit PRNG
F00:9172       * LOG		. LOG(<nexpr>)							* done fast cordic
F00:9173       * EXP		. EXP(<nexpr>)							* done fast cordic
F00:9174       * COS		. COS(<nexpr>)							* done fast cordic
F00:9175       * SIN		. SIN(<nexpr>)							* done fast cordic
F00:9176       * TAN		. TAN(<nexpr>)							* done fast cordic
F00:9177       * ATN		. ATN(<nexpr>)							* done fast cordic
F00:9178       * PEEK	. PEEK(<nexpr>)							* done
F00:9179       * DEEK	. DEEK(<nexpr>)							* done
F00:9180       * LEEK	. LEEK(<nexpr>)							* done
F00:9181       * LEN		. LEN(<sexpr>)							* done
F00:9182       * STR$	. STR$(<nexpr>)							* done
F00:9183       * VAL		. VAL(<sexpr>)							* done
F00:9184       * ASC		. ASC(<sexpr>)							* done
F00:9185       * UCASE$	. UCASE$(<sexpr>)							* done
F00:9186       * LCASE$	. LCASE$(<sexpr>)							* done
F00:9187       * CHR$	. CHR$(<nexpr>)							* done
F00:9188       * HEX$	. HEX$(<nexpr>)							* done
F00:9189       * BIN$	. BIN$(<nexpr>)							* done
F00:9190       * BTST	. BTST(<nexpr>,<nexpr>)						* done
F00:9191       * MAX		. MAX(<nexpr>[,<nexpr>[,<nexpr>]...])			* done
F00:9192       * MIN		. MIN(<nexpr>[,<nexpr>[,<nexpr>]...])			* done
