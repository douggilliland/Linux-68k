F00:0107       *************************************************************************************
F00:0108       *
F00:0109       * the following code is simulator specific, change to suit your system
F00:0110       
F00:0111       * output character to the console from register d0.b
F00:0112       
F00:0113       VEC_OUT
F00:0114               MOVEM.L  A0/D1,-(A7)    * Save working registers
               S02:00084802:  48 E7 40 80
F00:0115               LEA.L    SRA,A0     	* A0 points to console DUART
               S02:00084806:  41 F9 00 0F 00 02
F00:0116       TXNOTREADY
F00:0117               MOVE.B   (A0),D1        * Read DUART status
               S02:0008480C:  12 10
F00:0118               BTST     #2,D1          * Test Tx RDY bit
               S02:0008480E:  08 01 00 02
F00:0119               BEQ.S    TXNOTREADY     * Until DUART Tx ready
               S02:00084812:  67 F8
F00:0120               MOVE.B   D0,TBA       	* Write character to send
               S02:00084814:  13 C0 00 0F 00 06
F00:0121               MOVEM.L  (A7)+,A0/D1    * Restore working registers
               S02:0008481A:  4C DF 01 02
F00:0122               RTS
               S02:0008481E:  4E 75
F00:0123       
F00:0124       *************************************************************************************
F00:0125       *
F00:0126       * input a character from the console into register d0
F00:0127       * else return Cb=0 if there's no character available
F00:0128       
F00:0129       VEC_IN
F00:0130       	MOVEM.L  A0/D1,-(A7)    * Save working registers
               S02:00084820:  48 E7 40 80
F00:0131           LEA.L    SRA,A0      	* A0 points to console DUART
               S02:00084824:  41 F9 00 0F 00 02
F00:0132           MOVE.B   (A0),D1        * Read DUART status
               S02:0008482A:  12 10
F00:0133           BTST     #0,D1          * Test Rx Data Ready bit
               S02:0008482C:  08 01 00 00
F00:0134           BEQ.S    RXNOTREADY     * Branch If DUART Rx not ready
               S02:00084830:  67 10
F00:0135           MOVE.B   RBA,D0			* Read character received
               S02:00084832:  10 39 00 0F 00 06
F00:0136           MOVEM.L  (A7)+,A0/D1    * Restore working registers
               S02:00084838:  4C DF 01 02
F00:0137       	ORI.b	 #1,CCR	        * Set the carry, flag we got a byte
               S02:0008483C:  00 3C 00 01
F00:0138           RTS                     * Return
               S02:00084840:  4E 75
F00:0139       RXNOTREADY:
F00:0140           MOVEM.L  (A7)+,A0/D1    * Restore working registers
               S02:00084842:  4C DF 01 02
F00:0141       	ANDI.b	#$FE,CCR		* Clear the carry, flag character available
               S02:00084846:  02 3C 00 FE
F00:0142       	RTS
               S02:0008484A:  4E 75
