F00:9062       *************************************************************************************
F00:9063       * EhBASIC keywords quick reference list								*
F00:9064       *************************************************************************************
F00:9065       
F00:9066       * glossary
F00:9067       
F00:9068       *		<.>		  required
F00:9069       *		{.|.}		  one of required
F00:9070       *		[.]		  optional
F00:9071       *		...		  may repeat as last
F00:9072       
F00:9073       *		any		= anything
F00:9074       *		num		= number
F00:9075       *		state		= statement
F00:9076       *		n		= positive integer
F00:9077       *		str		= string
F00:9078       *		var		= variable
F00:9079       *		nvar		= numeric variable
F00:9080       *		svar		= string variable
F00:9081       *		expr		= expression
F00:9082       *		nexpr		= numeric expression
F00:9083       *		sexpr		= string expression
F00:9084       
F00:9085       * statement separator
F00:9086       
F00:9087       * :		. [<state>] : [<state>]						* done
F00:9088       
F00:9089       * number bases
F00:9090       
F00:9091       * %		. %<binary num>							* done
F00:9092       * $		. $<hex num>							* done
F00:9093       
F00:9094       * commands
F00:9095       
F00:9096       * END		. END									* done
F00:9097       * FOR		. FOR <nvar>=<nexpr> TO <nexpr> [STEP <nexpr>]		* done
F00:9098       * NEXT	. NEXT [<nvar>[,<nvar>]...]					* done
F00:9099       * DATA	. DATA [{num|["]str["]}[,{num|["]str["]}]...]		* done
F00:9100       * INPUT	. INPUT [<">str<">;] <var>[,<var>[,<var>]...]		* done
F00:9101       * DIM		. DIM <var>(<nexpr>[,<nexpr>[,<nexpr>]])			* done
F00:9102       * READ	. READ <var>[,<var>[,<var>]...]				* done
F00:9103       * LET		. [LET] <var>=<expr>						* done
F00:9104       * DEC		. DEC <nvar>[,<nvar>[,<nvar>]...]				* done
F00:9105       * GOTO	. GOTO <n>								* done
F00:9106       * RUN		. RUN [<n>]								* done
F00:9107       * IF		. IF <expr>{GOTO<n>|THEN<{n|comm}>}[ELSE <{n|comm}>]	* done
F00:9108       * RESTORE	. RESTORE [<n>]							* done
F00:9109       * GOSUB	. GOSUB <n>								* done
F00:9110       * RETURN	. RETURN								* done
F00:9111       * REM		. REM [<any>]							* done
F00:9112       * STOP	. STOP								* done
F00:9113       * ON		. ON <nexpr>{GOTO|GOSUB}<n>[,<n>[,<n>]...]		* done
F00:9114       * NULL	. NULL <nexpr>							* done
F00:9115       * INC		. INC <nvar>[,<nvar>[,<nvar>]...]				* done
F00:9116       * WAIT	. WAIT <nexpr>,<nexpr>[,<nexpr>]				* done
F00:9117       * LOAD	. LOAD [<sexpr>]							* done for sim
F00:9118       * SAVE	. SAVE [<sexpr>][,[<n>][-<n>]]				* done for sim
F00:9119       * DEF		. DEF FN<var>(<var>)=<expr>					* done
F00:9120       * POKE	. POKE <nexpr>,<nexpr>						* done
F00:9121       * DOKE	. DOKE <nexpr>,<nexpr>						* done
F00:9122       * LOKE	. LOKE <nexpr>,<nexpr>						* done
F00:9123       * CALL	. CALL <nexpr>							* done
F00:9124       * DO		. DO									* done
F00:9125       * LOOP	. LOOP [{WHILE|UNTIL}<nexpr>]					* done
F00:9126       * PRINT	. PRINT [{;|,}][<expr>][{;|,}[<expr>]...]			* done
F00:9127       * CONT	. CONT								* done
F00:9128       * LIST	. LIST [<n>][-<n>]						* done
F00:9129       * CLEAR	. CLEAR								* done
F00:9130       * NEW		. NEW									* done
F00:9131       * WIDTH	. WIDTH [<n>][,<n>]						* done
F00:9132       * GET		. GET <var>								* done
F00:9133       * SWAP	. SWAP <var>,<var>						* done
F00:9134       * BITSET	. BITSET <nexpr>,<nexpr>					* done
F00:9135       * BITCLR	. BITCLR <nexpr>,<nexpr>					* done
F00:9136       
F00:9137       * sub commands (may not start a statement)
F00:9138       
F00:9139       * TAB		. TAB(<nexpr>)							* done
F00:9140       * ELSE	. IF <expr>{GOTO<n>|THEN<{n|comm}>}[ELSE <{n|comm}>]	* done
F00:9141       * TO		. FOR <nvar>=<nexpr> TO <nexpr> [STEP <nexpr>]		* done
F00:9142       * FN		. FN <var>(<expr>)						* done
F00:9143       * SPC		. SPC(<nexpr>)							* done
F00:9144       * THEN	. IF <nexpr> {THEN <{n|comm}>|GOTO <n>}			* done
F00:9145       * NOT		. NOT <nexpr>							* done
F00:9146       * STEP	. FOR <nvar>=<nexpr> TO <nexpr> [STEP <nexpr>]		* done
F00:9147       * UNTIL	. LOOP [{WHILE|UNTIL}<nexpr>]					* done
F00:9148       * WHILE	. LOOP [{WHILE|UNTIL}<nexpr>]					* done
F00:9149       
F00:9150       * operators
F00:9151       
F00:9152       * +		. [expr] + <expr>							* done
F00:9153       * -		. [nexpr] - <nexpr>						* done
F00:9154       * *		. <nexpr> * <nexpr>						* done fast hardware
F00:9155       * /		. <nexpr> / <nexpr>						* done fast hardware
F00:9156       * ^		. <nexpr> ^ <nexpr>						* done
F00:9157       * AND		. <nexpr> AND <nexpr>						* done
F00:9158       * EOR		. <nexpr> EOR <nexpr>						* done
F00:9159       * OR		. <nexpr> OR <nexpr>						* done
F00:9160       * >>		. <nexpr> >> <nexpr>						* done
F00:9161       * <<		. <nexpr> << <nexpr>						* done
F00:9162       
F00:9163       * compare functions
F00:9164       
F00:9165       * <		. <expr> < <expr>							* done
F00:9166       * =		. <expr> = <expr>							* done
F00:9167       * >		. <expr> > <expr>							* done
F00:9168       
F00:9169       * functions
F00:9170       
F00:9171       * SGN		. SGN(<nexpr>)							* done
F00:9172       * INT		. INT(<nexpr>)							* done
F00:9173       * ABS		. ABS(<nexpr>)							* done
F00:9174       * USR		. USR(<expr>)							* done
F00:9175       * FRE		. FRE(<expr>)							* done
F00:9176       * POS		. POS(<expr>)							* done
F00:9177       * SQR		. SQR(<nexpr>)							* done fast shift/sub
F00:9178       * RND		. RND(<nexpr>)							* done 32 bit PRNG
F00:9179       * LOG		. LOG(<nexpr>)							* done fast cordic
F00:9180       * EXP		. EXP(<nexpr>)							* done fast cordic
F00:9181       * COS		. COS(<nexpr>)							* done fast cordic
F00:9182       * SIN		. SIN(<nexpr>)							* done fast cordic
F00:9183       * TAN		. TAN(<nexpr>)							* done fast cordic
F00:9184       * ATN		. ATN(<nexpr>)							* done fast cordic
F00:9185       * PEEK	. PEEK(<nexpr>)							* done
F00:9186       * DEEK	. DEEK(<nexpr>)							* done
F00:9187       * LEEK	. LEEK(<nexpr>)							* done
F00:9188       * LEN		. LEN(<sexpr>)							* done
F00:9189       * STR$	. STR$(<nexpr>)							* done
F00:9190       * VAL		. VAL(<sexpr>)							* done
F00:9191       * ASC		. ASC(<sexpr>)							* done
F00:9192       * UCASE$	. UCASE$(<sexpr>)							* done
F00:9193       * LCASE$	. LCASE$(<sexpr>)							* done
F00:9194       * CHR$	. CHR$(<nexpr>)							* done
F00:9195       * HEX$	. HEX$(<nexpr>)							* done
F00:9196       * BIN$	. BIN$(<nexpr>)							* done
F00:9197       * BTST	. BTST(<nexpr>,<nexpr>)						* done
F00:9198       * MAX		. MAX(<nexpr>[,<nexpr>[,<nexpr>]...])			* done
F00:9199       * MIN		. MIN(<nexpr>[,<nexpr>[,<nexpr>]...])			* done
F00:9200       * PI		. PI									* done
F00:9201       * TWOPI	. TWOPI								* done
F00:9202       * VARPTR	. VARPTR(<var>)							* done
F00:9203       * SADD	. SADD(<svar>)							* done
F00:9204       * LEFT$	. LEFT$(<sexpr>,<nexpr>)					* done
F00:9205       * RIGHT$	. RIGHT$(<sexpr>,<nexpr>)					* done
